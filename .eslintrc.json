{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["prettier"],
  "extends": ["prettier", "eslint:recommended"],

  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
//    "ecmaFeatures": {
//      "arrowFunctions": true,
//      "modules": true,
//      "defaultParams": true,
//      "forOf": true
//    }
  },
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
